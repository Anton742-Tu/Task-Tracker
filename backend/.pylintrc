[MASTER]
load-plugins=pylint_django

[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    import-error,
    no-member,
    not-callable,
    invalid-name,
    duplicate-code,
    too-many-arguments,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    duplicate-code

[DJANGO]
settings-module=config.settings

[FORMAT]
max-line-length=88

[TYPECHECK]
ignored-modules=
    rest_framework,
    django

[DESIGN]
max-attributes=15
min-public-methods=1

[BASIC]
good-names=
    i,j,k,ex,Run,_
