[run]
source = .
omit =
    */tests/*
    */migrations/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */env/*
    manage.py
    config/wsgi.py
    config/asgi.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    @property
    class .*\bProtocol\b:
    self\.logger\.

omit_empty_files = true
show_missing = true
skip_covered = false