[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    migrations,
    static,
    media,
    .tox,
    .pytest_cache,
    build,
    dist
per-file-ignores =
    __init__.py:F401
    tests/*:S101
    */migrations/*:F401
ignore = 
    W504,
    E501
